<Window x:Class="DokaponFileReader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DokaponFileReader"
        mc:Ignorable="d"
        Title="Dokapon File Reader" Height="723" Width="1060">
    <Grid>
        <Menu x:Name="MainMenu" HorizontalAlignment="Center" Height="20" VerticalAlignment="Top" Width="1040">
            <MenuItem x:Name="OpenMenuItem" FontWeight="Bold" Header="Open" Click="OpenMenuItem_Click"/>
            <MenuItem x:Name="SaveMenuItem" Header="Save" FontWeight="Bold" Click="SaveMenuItem_Click"/>
            <MenuItem x:Name="CloseMenuItem" FontWeight="Bold" Header="Exit" ItemsSource="{Binding ClickEvent, ElementName=CloseMenuItem}" Click="CloseMenuItem_Click"/>
        </Menu>
        <TabControl HorizontalAlignment="Left" Height="632" Margin="23,43,0,0" VerticalAlignment="Top" Width="999">
            <TabItem x:Name="JobDataTab" Header="Jobs">
                <Grid Background="#FFE5E5E5">
                    <TabControl>
                        <TabItem x:Name="StartingStats" Header="Starting Stats">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="JobStartingStats" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Job Name"  Binding="{Binding jobName, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Sex"  Binding="{Binding sex, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="AT"  Binding="{Binding startingAttack, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="DF"  Binding="{Binding startingDefense, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="MG"  Binding="{Binding startingMagic, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="SP"  Binding="{Binding startingSpeed, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="HP"  Binding="{Binding startingHP, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Items"  Binding="{Binding bagItemSpace, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Field Magic"  Binding="{Binding fieldMagicSpace, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Description"  Binding="{Binding jobDescription, UpdateSourceTrigger=PropertyChanged}"/>

                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="LevelUp" Header="Level Up">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="JobLevelUpStats" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Job Name"  Binding="{Binding jobName, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Sex"  Binding="{Binding sex, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="AT"  Binding="{Binding levelUpAttack, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="DF"  Binding="{Binding levelUpDefense, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="MG"  Binding="{Binding levelUpMagic, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="SP"  Binding="{Binding levelUpSpeed, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="HP"  Binding="{Binding levelUpHP, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Battles to Level"  Binding="{Binding battlesToLevel, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="JobMastery" HorizontalAlignment="Center" Height="20" Header="Mastery" VerticalAlignment="Center" Width="54">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="JobMasteryStats" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Job Name"  Binding="{Binding jobName, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Sex"  Binding="{Binding sex, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="AT"  Binding="{Binding masteryAttack, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="DF"  Binding="{Binding masteryDefense, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="MG"  Binding="{Binding masteryMagic, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="SP"  Binding="{Binding masterySpeed, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="HP"  Binding="{Binding masteryHP, UpdateSourceTrigger=PropertyChanged}"/>

                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="SalaryData" HorizontalAlignment="Center" Height="20" Header="Salary" VerticalAlignment="Center" Width="54">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="SalaryStats" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Job Name"  Binding="{Binding jobName, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Sex"  Binding="{Binding sex, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Salary"  Binding="{Binding startingSalary, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Salary Multiplier"  Binding="{Binding levelUpSalaryMultiplier, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Bonus Count"  Binding="{Binding bonusRequirementCount, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Bonus Multiplier"  Binding="{Binding bonusMultiplier, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="SkillData" HorizontalAlignment="Center" Height="20" Header="Skills" VerticalAlignment="Center" Width="54">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="SkillStats" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Job Name"  Binding="{Binding jobName, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Sex"  Binding="{Binding sex, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Level 4 Skill"  Binding="{Binding level4BattleSkill, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Level 6 Skill"  Binding="{Binding level6BattleSkill, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Passive"  Binding="{Binding jobPassiveName, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Activation Rate"  Binding="{Binding jobPassiveActivationRate, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Passive Effect"  Binding="{Binding jobPassiveDescription, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="RequirementData" HorizontalAlignment="Center" Height="20" Header="Requirements" VerticalAlignment="Top" Width="87" Margin="0,-1,0,0">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="Requirements" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Job Name"  Binding="{Binding jobName, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Sex"  Binding="{Binding sex, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Mastery 1"  Binding="{Binding masteryRequirement[0], UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Mastery 2"  Binding="{Binding masteryRequirement[1], UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Mastery 3"  Binding="{Binding masteryRequirement[2], UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Item Required"  Binding="{Binding itemRequirement, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                    </TabControl>
                </Grid>
            </TabItem>
            <TabItem x:Name="MonsterDataTab" Header="Monsters">
