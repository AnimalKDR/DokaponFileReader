                                        <DataGridTextColumn Header="Icon ID"  Binding="{Binding iconID, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Sort Index"  Binding="{Binding sortIndex, UpdateSourceTrigger=PropertyChanged}"/>
