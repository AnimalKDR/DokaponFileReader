<Window x:Class="DokaponFileReader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DokaponFileReader"
        mc:Ignorable="d"
        Title="Dokapon File Reader" Height="723" Width="1060">
    <Grid>
        <Menu x:Name="MainMenu" HorizontalAlignment="Center" Height="20" VerticalAlignment="Top" Width="1040">
            <MenuItem x:Name="OpenMenuItem" FontWeight="Bold" Header="Open" Click="OpenMenuItem_Click"/>
            <MenuItem x:Name="SaveMenuItem" Header="Save" FontWeight="Bold" Click="SaveMenuItem_Click"/>
            <MenuItem x:Name="CloseMenuItem" FontWeight="Bold" Header="Exit" ItemsSource="{Binding ClickEvent, ElementName=CloseMenuItem}" Click="CloseMenuItem_Click"/>
        </Menu>
        <TabControl HorizontalAlignment="Left" Height="632" Margin="23,43,0,0" VerticalAlignment="Top" Width="999">
            <TabItem x:Name="JobDataTab" Header="Jobs">
                <Grid Background="#FFE5E5E5">
                    <TabControl>
                        <TabItem x:Name="StartingStats" Header="Starting Stats">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="JobStartingStats" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Job Name"  Binding="{Binding jobName.name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="AT"  Binding="{Binding startingAttack, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="DF"  Binding="{Binding startingDefense, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="MG"  Binding="{Binding startingMagic, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="SP"  Binding="{Binding startingSpeed, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="HP"  Binding="{Binding startingHP, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Items"  Binding="{Binding bagItemSpace, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Field Magic"  Binding="{Binding fieldMagicSpace, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Description"  Binding="{Binding jobDescription, UpdateSourceTrigger=PropertyChanged}"/>

                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="LevelUp" Header="Level Up">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="JobLevelUpStats" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Job Name"  Binding="{Binding jobName.name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="AT"  Binding="{Binding levelUpAttack, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="DF"  Binding="{Binding levelUpDefense, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="MG"  Binding="{Binding levelUpMagic, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="SP"  Binding="{Binding levelUpSpeed, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="HP"  Binding="{Binding levelUpHP, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Battles to Level"  Binding="{Binding battlesToLevel, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="JobMastery" HorizontalAlignment="Center" Height="20" Header="Mastery" VerticalAlignment="Center" Width="54">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="JobMasteryStats" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Job Name"  Binding="{Binding jobName.name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="AT"  Binding="{Binding masteryAttack, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="DF"  Binding="{Binding masteryDefense, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="MG"  Binding="{Binding masteryMagic, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="SP"  Binding="{Binding masterySpeed, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="HP"  Binding="{Binding masteryHP, UpdateSourceTrigger=PropertyChanged}"/>

                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="SalaryData" HorizontalAlignment="Center" Height="20" Header="Salary" VerticalAlignment="Center" Width="54">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="SalaryStats" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Job Name"  Binding="{Binding jobName.name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Salary"  Binding="{Binding startingSalary, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Salary Multiplier"  Binding="{Binding levelUpSalaryMultiplier, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Bonus Count"  Binding="{Binding bonusRequirementCount, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Bonus Multiplier"  Binding="{Binding bonusMultiplier, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="SkillData" HorizontalAlignment="Center" Height="20" Header="Skills" VerticalAlignment="Center" Width="54">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="SkillStats" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Job Name"  Binding="{Binding jobName.name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridComboBoxColumn x:Name="Level2BattleSkill" Header="Level 2 Skill" SelectedValueBinding="{Binding level2BattleSkill, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="name"/>
                                        <DataGridComboBoxColumn x:Name="Level4BattleSkill" Header="Level 4 Skill" SelectedValueBinding="{Binding level4BattleSkill, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="name"/>
                                        <DataGridComboBoxColumn x:Name="PassiveSkill" Header="Passive Skill" SelectedValueBinding="{Binding passiveSkill, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="name"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="RequirementData" HorizontalAlignment="Center" Height="20" Header="Requirements" VerticalAlignment="Top" Width="87" Margin="0,-1,0,0">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="Requirements" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Job Name"  Binding="{Binding jobName.name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridComboBoxColumn x:Name="MasteryRequirement1" Header="Mastery 1" SelectedValueBinding="{Binding masteryRequirement[0], UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="name"/>
                                        <DataGridComboBoxColumn x:Name="MasteryRequirement2" Header="Mastery 2" SelectedValueBinding="{Binding masteryRequirement[1], UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="name"/>
                                        <DataGridComboBoxColumn x:Name="MasteryRequirement3" Header="Mastery 3" SelectedValueBinding="{Binding masteryRequirement[2], UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="name"/>
                                        <DataGridComboBoxColumn x:Name="ItemRequirement" Header="Item Required" SelectedValueBinding="{Binding itemRequirement, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="name"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                    </TabControl>
                </Grid>
            </TabItem>
            <TabItem x:Name="MonsterDataTab" Header="Monsters">
                <Grid Background="#FFE5E5E5">
                    <TabControl>
                        <TabItem x:Name="GeneralMonsterData" Header="General">
                            <Grid Background="#FFE5E5E5">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="169*"/>
                                    <ColumnDefinition Width="818*"/>
                                </Grid.ColumnDefinitions>
                                <DataGrid x:Name="GeneralData" ItemsSource="{Binding}" AutoGenerateColumns="False" Grid.ColumnSpan="2">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Level"  Binding="{Binding level, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="AT"  Binding="{Binding attack, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="DF"  Binding="{Binding defense, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="MG"  Binding="{Binding magic, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="SP"  Binding="{Binding speed, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="HP"  Binding="{Binding hp, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="AT"  Binding="{Binding specialTypeAttack, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="DF"  Binding="{Binding specialTypeDefense, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="MG"  Binding="{Binding specialTypeMagic, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="SP"  Binding="{Binding specialTypeSpeed, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="HP"  Binding="{Binding specialTypeHP, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Monster Type"  Binding="{Binding monsterType, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Voice ID"  Binding="{Binding voiceID, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="MonsterCombatData" Header="Combat">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="CombatData" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridComboBoxColumn x:Name="MonsterOffensiveMagic" Header="Offensive Magic" SelectedValueBinding="{Binding offensiveMagic, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="name"/>
                                        <DataGridComboBoxColumn x:Name="MonsterDefensiveMagic" Header="Defensive Magic" SelectedValueBinding="{Binding defensiveMagic, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="name"/>
                                        <DataGridComboBoxColumn x:Name="MonsterBattleSkill" Header="Battle Skill" SelectedValueBinding="{Binding battleSkill, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="name"/>
                                        <DataGridTextColumn Header="AI Table"  Binding="{Binding aiIndex, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="MonsterRewardData" HorizontalAlignment="Center" Height="20" Header="Rewards" VerticalAlignment="Top" Width="71" Margin="0,-1,0,0">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="RewardData" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="XP"  Binding="{Binding experience, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Gold"  Binding="{Binding gold, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Times Week?"  Binding="{Binding dynamicGold, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridComboBoxColumn x:Name="DropItem1" Header="Item 1" SelectedValueBinding="{Binding dropItem[0], UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="name"/>
                                        <DataGridTextColumn Header="Item 1 Chance"  Binding="{Binding dropItemChance[0], UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridComboBoxColumn x:Name="DropItem2" Header="Item 2" SelectedValueBinding="{Binding dropItem[1], UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="name"/>
                                        <DataGridTextColumn Header="Item 2 Chance"  Binding="{Binding dropItemChance[1], UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="MonsterDescriptionData" HorizontalAlignment="Center" Height="20" Header="Description" VerticalAlignment="Top" Width="86" Margin="0,-1,0,0">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="DescriptionData" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                    </TabControl>
                </Grid>
            </TabItem>
            <TabItem x:Name="ItemDataTab" HorizontalAlignment="Center" Height="20" Header="Items" VerticalAlignment="Center" Width="54">
                <Grid Background="#FFE5E5E5">
                    <TabControl>
                        <TabItem x:Name="WeaponDataTab" Header="Weapons">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="WeaponsData" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Price"  Binding="{Binding price, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="AT"  Binding="{Binding attack, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="DF"  Binding="{Binding defense, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="MG"  Binding="{Binding magic, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="SP"  Binding="{Binding speed, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="HP"  Binding="{Binding hp, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Bonus Class"  Binding="{Binding bonusClass, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Activation Rate"  Binding="{Binding activationRate, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Icon ID"  Binding="{Binding iconID, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Attack Type"  Binding="{Binding attackAnimation, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="ShieldDataTab" Header="Shields">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="ShieldDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Price"  Binding="{Binding price, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="AT"  Binding="{Binding attack, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="DF"  Binding="{Binding defense, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="MG"  Binding="{Binding magic, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="SP"  Binding="{Binding speed, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="HP"  Binding="{Binding hp, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Activation Rate"  Binding="{Binding activationRate, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Icon ID"  Binding="{Binding iconID, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="AccessoryDataTab" HorizontalAlignment="Center" Height="20" Header="Accessories" VerticalAlignment="Top" Width="84" Margin="0,-1,0,0">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="AccessoryDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Price"  Binding="{Binding price, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="AT"  Binding="{Binding attack, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="DF"  Binding="{Binding defense, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="MG"  Binding="{Binding magic, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="SP"  Binding="{Binding speed, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="HP"  Binding="{Binding hp, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Activation Rate"  Binding="{Binding activationRate, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Icon ID"  Binding="{Binding iconID, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="HairstyleDataTab" HorizontalAlignment="Center" Height="20" Header="HairStyles" VerticalAlignment="Top" Width="72" Margin="0,-1,0,0">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="HairstyleDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Sex"  Binding="{Binding sex, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Hair Type ID"  Binding="{Binding hairTypeID, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Icon ID"  Binding="{Binding iconID, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="BagItemDataTab" HorizontalAlignment="Center" Height="20" Header="Bag Items" VerticalAlignment="Top" Width="76" Margin="0,-1,0,0">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="BagItemDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Price"  Binding="{Binding price, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Item Type"  Binding="{Binding item.itemType, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Icon ID"  Binding="{Binding iconID, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="LocalItemDataTab" HorizontalAlignment="Center" Height="20" Header="Local Items" VerticalAlignment="Top" Width="76" Margin="0,-1,0,0">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="LocalItemDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Location"  Binding="{Binding location, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Price"  Binding="{Binding price, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Gift Value"  Binding="{Binding localItemValue, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Spawn Rate"  Binding="{Binding spawnRate, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Icon ID"  Binding="{Binding iconID, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Deliver Description"  Binding="{Binding deliverDescription, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="OffensiveMagicDataTab" HorizontalAlignment="Center" Height="20" Header="Offensive Magic" VerticalAlignment="Top" Width="102" Margin="0,-1,0,0">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="OffensiveMagicDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Price"  Binding="{Binding price, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Power"  Binding="{Binding power, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Magic Type"  Binding="{Binding magicType, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Effect Type"  Binding="{Binding effectType, StringFormat={}0x{0:X4}, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Icon ID"  Binding="{Binding iconID, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Sort Index"  Binding="{Binding sortIndex, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="DefensiveMagicDataTab" HorizontalAlignment="Center" Height="20" Header="Defensive Magic" VerticalAlignment="Top" Width="106" Margin="0,-1,0,0">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="DefensiveMagicDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Price"  Binding="{Binding price, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Power"  Binding="{Binding power, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Magic Type"  Binding="{Binding magicType, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Effect Type"  Binding="{Binding effectType, StringFormat={}0x{0:X4}, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Icon ID"  Binding="{Binding iconID, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Sort Index"  Binding="{Binding sortIndex, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="FieldMagicDataTab" HorizontalAlignment="Center" Height="20" Header="Field Magic" VerticalAlignment="Top" Width="86" Margin="0,-1,0,0">
                            <Grid Background="#FFE5E5E5">
                                <DataGrid x:Name="FieldMagicDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Price"  Binding="{Binding price, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Power"  Binding="{Binding power, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Magic Type"  Binding="{Binding magicType, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Effect Type"  Binding="{Binding effectType, StringFormat={}0x{0:X4}, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Icon ID"  Binding="{Binding iconID, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="BattleSkillDataTab" HorizontalAlignment="Center" Height="20" Header="Battle Skills" VerticalAlignment="Top" Width="82" Margin="0,-1,0,0">
                            <DataGrid x:Name="BattleSkillDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                    <DataGridTextColumn Header="Activation Rate"  Binding="{Binding activationRate, UpdateSourceTrigger=PropertyChanged}"/>
                                    <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                </DataGrid.Columns>
                            </DataGrid>
                        </TabItem>
                        <TabItem x:Name="PassiveSkillDataTab" HorizontalAlignment="Center" Height="20" Header="Passive Skills" VerticalAlignment="Top" Width="82" Margin="0,-1,0,0">
                            <DataGrid x:Name="PassiveSkillDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                    <DataGridTextColumn Header="Activation Rate"  Binding="{Binding activationRate, UpdateSourceTrigger=PropertyChanged}"/>
                                    <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                </DataGrid.Columns>
                            </DataGrid>
                        </TabItem>
                        <TabItem x:Name="DarkArtDataTab" HorizontalAlignment="Center" Height="20" Header="Dark Arts" VerticalAlignment="Top" Width="82" Margin="0,-1,0,0">
                            <DataGrid x:Name="DarkArtDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                    <DataGridTextColumn Header="Cost"  Binding="{Binding cost, UpdateSourceTrigger=PropertyChanged}"/>
                                    <DataGridTextColumn Header="Description"  Binding="{Binding description, UpdateSourceTrigger=PropertyChanged}"/>
                                </DataGrid.Columns>
                            </DataGrid>
                        </TabItem>
                    </TabControl>
                </Grid>
            </TabItem>
            <TabItem HorizontalAlignment="Center" Height="20" Header="NPCs" VerticalAlignment="Center" Width="54">
                <Grid x:Name="NPCDataTab" Background="#FFE5E5E5">
                    <TabControl>
                        <TabItem x:Name="NPCNameDataTab" Header="NPC Names">
                            <DataGrid x:Name="NPCNameDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Name"  Binding="{Binding name, UpdateSourceTrigger=PropertyChanged}"/>
                                </DataGrid.Columns>
                            </DataGrid>
                        </TabItem>
                        <TabItem x:Name="DialogueDataTab" Header="Dialogue">
                            <DataGrid x:Name="DialogueDatas" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Dialogue"  Binding="{Binding dialogue, UpdateSourceTrigger=PropertyChanged}"/>
                                </DataGrid.Columns>
                            </DataGrid>
                        </TabItem>
                    </TabControl>
                </Grid>
            </TabItem>
            <TabItem x:Name="MiscTab" HorizontalAlignment="Center" Height="20" Header="Miscellaneous" VerticalAlignment="Top" Width="102" Margin="0,-1,0,0">
                <Grid Background="#FFE5E5E5">
                    <TabControl>
                        <TabItem x:Name="CombatFormulaDataTab" Header="Combat Formulas">
                            <Grid Background="#FFE5E5E5">
