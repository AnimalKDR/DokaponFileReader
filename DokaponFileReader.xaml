                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="BagItemDataTab" HorizontalAlignment="Center" Height="20" Header="Bag Items" VerticalAlignment="Top" Width="76" Margin="0,-1,0,0">
